      "search_tab": "公式搜索",
      "search_button": "搜索",
      "clear_button": "清除",
      "stop_button": "停止",
      "result_table": "搜索结果"
    }
  },
  "search": {
    "formula_count": 100,
    "data_periods": 200,
    "fixed_code_count": 5,
    "plan_cycle": 1,
    "min_accuracy": 57,
    "search_timeout": 300,
    "result_wait_timeout": 60,
    "max_results": 5,
    "parameters": {
      "formula_type": "自定义",
      "data_source": "历史数据",
      "analysis_method": "统计分析"
    },
    "ocr_stats_region": {
      "width": 280,
      "height": 550,
      "anchor_gap": 10,
      "offset_x": 845,
      "offset_y": 50,
      "inner_offset_x": 20
    },
    "tesseract_cmd": "D:/Tools/Tesseract/tesseract.exe"
  },
  "api": {
    "links_file": "d:/PC-Test/docs/链接.md",
    "base_url": "https://kjapi.com",
    "endpoints": {
      "history_data": "/hall/hallhistory/txffcqiqu/ksffc",
      "date_data": "/hallhistoryDetail/txffcqiqu/{date}"
