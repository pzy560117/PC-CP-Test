    "timeout": 10,
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
      "Accept": "application/json",
      "Content-Type": "application/json",
      "Referer": "https://kjapi.com"
    },
    "rate_limit": {
      "requests_per_minute": 30,
      "burst_limit": 5,
      "delay_seconds": 1.5
    }
  },
  "recommendation": {
    "source_file": "./data/recommendations.txt",
    "encoding": "utf-8",
    "max_records": 5,
    "history_filename": "comparison_history.jsonl"
  },
  "lottery": {
    "wait_for_new_result": true,
    "poll_interval": 5,
    "max_wait_seconds": 90,
    "force_ocr_only": true,
    "ocr": {
      "enabled": true,
      "region": [200, 120, 460, 420],
      "languages": ["ch_sim", "en"],
      "period_length": 12,
      "numbers_length": 5,
      "max_retries": 3,
      "retry_interval": 2,
      "debug_capture_path": "./logs/lottery_ocr_capture.png",
      "window_title": "计划接口-奇趣腾讯分分彩",
      "window_region": {
        "offset_x": 40,
        "offset_y": 100,
        "width": 520,
        "height": 260,
        "anchor_gap": 20
      }
    }
  },
  "mysql": {
    "enabled": true,
    "host": "127.0.0.1",
    "port": 3306,
    "user": "root",
    "password": "560117",
    "database": "lottery",
