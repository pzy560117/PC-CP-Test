  "browser": {
    "type": "chrome",
    "headless": false,
    "window_size": {
      "width": 1280,
      "height": 720
    },
    "page_load_timeout": 30,
    "element_wait_timeout": 10,
    "implicit_wait": 5,
    "options": [
      "--disable-infobars",
      "--disable-extensions",
      "--disable-gpu",
      "--no-sandbox"
    ]
  },
  "target_app": {
    "name": "奇趣腾讯分分彩",
    "executable_path": "D:/software/CpzyrjCom.exe",
    "window_title": "奇趣腾讯分分彩",
    "launch_timeout": 30,
