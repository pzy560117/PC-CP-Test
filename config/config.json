    "backup_path": "./data/backup",
    "cache_enabled": true,
    "cache_size": "100MB",
    "auto_cleanup": true,
    "retention_days": 30
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "./logs/app.log",
    "max_size": "10MB",
